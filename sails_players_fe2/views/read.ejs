<%- partial ('partials/statusmessage.ejs') %>

<legend>Contacts</legend>
<table class="table table-striped table-hover" id="contactTable" style="width:100%">
  <thead>
    <tr>
      <th>Title</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Company</th>
      <th>Address Type</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip Code</th>
      <th>Phone Type</th>
      <th>Phone Number</th>

    </tr>
  </thead>
  <tbody>
  <% if(typeof contact != "undefined" && cotact.length > 0){ %>
    <% contact.forEach(function(contact){ %>
      <tr>
        <td><%= contact.firstName %></td>
        <td><%= contact.lastName %></td>
        <td><%= contact.title %></td>
        <td><%= contact.position %></td>
      </tr>
    <% }) %>
  <% } %>
  </tbody>
</table>
